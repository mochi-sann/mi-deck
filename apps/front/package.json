{
  "name": "@mi-deck/front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "generate-routes": "tsr generate",
    "watch-routes": "tsr watch",
    "prepare": "panda codegen",
    "predev": "npx swagger2openapi --yaml ../server-go/docs/swagger.json > ./src/lib/api/swagger_v3.yaml  && npx openapi-typescript ./src/lib/api/swagger_v3.yaml -o ./src/lib/api/type.ts",
    "dev": "vite",
    "build": "tsr generate && tsc -b && vite build",
    "build:analyzer": "BUNDLE_ANALYZE=true pnpm run build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "vite preview"
  },
  "dependencies": {
    "@ark-ui/react": "^4.5.0",
    "@park-ui/panda-preset": "^0.42.0",
    "@tanstack/react-query": "^5.62.3",
    "@tanstack/react-router": "^1.91.2",
    "lucide-react": "^0.469.0",
    "miauth-js": "^0.0.6",
    "misskey-js": "2024.11.0",
    "openapi-fetch": "^0.13.3",
    "openapi-react-query": "^0.2.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.0",
    "react-query-auth": "^2.4.3",
    "usehooks-ts": "^3.1.0",
    "valibot": "1.0.0-beta.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@pandacss/dev": "^0.49.0",
    "@tanstack/react-query-devtools": "^5.62.10",
    "@tanstack/router-cli": "^1.87.7",
    "@tanstack/router-devtools": "^1.91.2",
    "@tanstack/router-plugin": "^1.91.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "globals": "^15.12.0",
    "openapi-typescript": "^7.4.4",
    "swagger2openapi": "^7.0.8",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1",
    "vite-bundle-analyzer": "^0.15.2",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
