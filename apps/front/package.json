    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@vitejs/plugin-react-swc": "^3.8.1",
    "@vitest/browser": "3.1.2",
    "@vitest/coverage-v8": "3.1.2",
    "@vitest/ui": "^3.0.0",
    "concurrently": "^9.1.2",
    "globals": "^15.15.0",
    "http-server": "^14.1.1",
    "jsdom": "^25.0.0",
    "msw": "^2.7.6",
    "openapi-typescript": "^7.6.1",
    "orval": "^7.9.0",
