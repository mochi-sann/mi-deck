{
  "name": "@mi-deck/react-mfm",
  "description": "MFM for React",
  "dependencies": {
    "@twemoji/api": "16.0.1",
    "dexie": "^4.0.11",
    "jotai": "^2.12.5",
    "katex": "0.16.22",
    "mfm-js": "0.25.0",
    "shiki": "3.12.2"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "7.47.5",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/katex": "0.16.7",
    "@types/react": "^19.1.9",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "@vitest/coverage-v8": "4.0.6",
    "fake-indexeddb": "^6.0.1",
    "jsdom": "^25.0.1",
    "lucide-react": "^0.513.0",
    "tsup": "8.5.0",
    "vite": "^7.0.6",
    "vitest": "^4.0.0"
  },
  "exports": {
    ".": "./dist/index.js",
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "peerDependencies": {
    "react": ">=19.0.0"
  },
  "scripts": {
    "build": "tsup --minify --dts",
    "check": "tsc",
    "dev": "tsup --watch",
    "fmt": "prettier -w .",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --run --coverage"
  },
  "tsup": {
    "entry": [
      "src/index.tsx",
      "src/Node.tsx",
      "src/utils.ts",
      "src/components/*.tsx",
      "src/style.css"
    ],
    "format": "esm",
    "sourcemap": true,
    "splitting": true,
    "clean": true
  },
  "type": "module",
  "types": "./dist/index.d.ts"
}
